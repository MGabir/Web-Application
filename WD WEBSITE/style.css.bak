*{
	margin:0;
	padding: 0;
	font-family: sansation;
}
.header{
	min-height: 100vh;
	width:100%;
	background-image: linear-gradient(rgba(4,9,30,0.7),rgba(4,9,30,0.7)), url(images/atif.aslam.JPG);
	background-position: centre;
	background-size: cover;
	position: relative;
}
nav{
	display:flex;
	padding:2% 6%;
	justify-content: space-between;
	align-items: centre;
}
nav img{
	width: 150px;
}
.nav-links{
	flex:1;
	text-align: right;
}
.nav-links ul li{
	list-style: none;
	display: inline-block:
	padding: 8px 12px;
	position: relative;
}
.nav-links ul li a{
	color: #e97c62;
	text-decoration: none;
	font-size; 13px;
}
.nav-links ul li::after{
	content'';
	width: 0%;
	height:2px;
	background: #deddd3;
	display: block;
	margin: auto;
}
.nav-links ul li:hover::after{
	width:100%;
}